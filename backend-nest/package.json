{
  "name": "patients-influence-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "================================== NEST CORE ===================================": "",
    "oldstart": "nest start",
    "start": "node dist/src/main.js",
    "start:dev": "npm run env:dev -- \"nest start --watch\"",
    "start:dev:hmr": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:dev:hmr-nocli": "webpack -- config webpack.config.js --watch",
    "start:debug": "npm run env:dev -- \"nest start --debug --watch\"",
    "start:prod": "npm run env:prod -- \"node dist/src/main\"",
    "build": "npm run env:prod -- \"nest build\"",
    "build:prod": "npm run env:prod -- nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "npm run env:test -- \"jest --watch\"",
    "test:cov": "npm run env:test -- \"jest --coverage\"",
    "test:debug": "npm run env:test -- \"node --inspect-brk -r -r ts-node/register node_modules/.bin/jest --runInBand\"",
    "test:e2e": "npm run env:test -- \"jest --config ./test/jest-e2e.json\"",
    "pretest:e2e": "npm run docker:test:db:restart",
    "prepare": "husky install",
    "================================= ENVIRONMENT ==================================": "",
    "env:dev": "dotenv -e .env.prod",
    "env:prod": "dotenv -e .env.prod",
    "env:test": "dotenv -e ./env/.env.test",
    "=================================== PRISMA =====================================": "",
    "prisma:dev:generate": "npm run env:dev -- \"prisma generate\"",
    "prisma:prod:generate": "npm run env:prod -- prisma generate",
    "prisma:test:generate": "npm run env:test -- \"prisma generate\"",
    "prisma:dev:studio": "npm run env:dev -- \"prisma studio\"",
    "prisma:prod:studio": "npm run env:prod -- \"prisma studio\"",
    "prisma:test:studio": "npm run env:test -- \"prisma studio\"",
    "prisma:dev:migrate": "npm run env:dev -- \"prisma migrate dev\"",
    "prisma:dev:migrate:create": "npm run env:dev -- \"prisma migrate dev --create-only\"",
    "prisma:prod:migrate": "npm run env:prod -- \"prisma migrate deploy\"",
    "prisma:test:migrate": "npm run env:test -- \"prisma migrate dev\"",
    "prisma:dev:seed": "npm run env:dev -- ts-node ./prisma/seeders/index.ts",
    "prisma:prod:seed": "npm run env:prod -- ts-node ./prisma/seeders/index.ts",
    "prisma:test:seed": "npm run env:test -- \"ts-node ./prisma/seeders/index.ts\"",
    "================================ DOCKER SCRIPTS ================================": "",
    "docker:dev:up": "npm run env:dev -- \"docker compose -f ./docker/docker-compose.dev.yml up -d\"",
    "docker:prod:up": "npm run env:prod -- \"docker compose -f ./docker/docker-compose.prod.yml up -d\"",
    "docker:test:up": "npm run env:test -- \"docker compose -f ./docker/docker-compose.test.yml up -d\"",
    "docker:dev:down": "npm run env:dev -- \"docker compose -f ./docker/docker-compose.dev.yml down\"",
    "docker:prod:down": "npm run env:prod -- \"docker compose -f ./docker/docker-compose.prod.yml down\"",
    "docker:test:down": "npm run env:test -- \"docker compose -f ./docker/docker-compose.test.yml down\"",
    "docker:test:db:remove": "npm run env:test -- \"docker compose -f ./docker/docker-compose.test.yml rm test-db -f -s -v\"",
    "docker:test:db:restart": "npm run docker:test:db:remove && npm run docker:test:up && npm run prisma:test:migrate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.369.0",
    "@aws-sdk/s3-request-presigner": "^3.369.0",
    "@casl/ability": "^6.4.0",
    "@casl/prisma": "^1.3.3",
    "@faker-js/faker": "^8.0.1",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.4.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/websockets": "^9.4.0",
    "@prisma/client": "^4.14.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/lodash": "^4.14.195",
    "axios": "^1.3.6",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compromise": "^14.9.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.30.0",
    "decimal.js": "^10.4.3",
    "emoji-regex": "^10.2.1",
    "helmet": "^6.1.5",
    "lodash": "^4.17.21",
    "nest-winston": "^1.9.1",
    "nestjs-i18n": "^10.2.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "simple-statistics": "^7.8.3",
    "socket.io": "^4.6.1",
    "ts-mixer": "^6.0.3",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "28.1.3",
    "jira-prepare-commit-msg": "^1.7.2",
    "prettier": "^2.3.2",
    "prisma": "^4.13.0",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
